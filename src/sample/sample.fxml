<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>


<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="sample.Controller">
   <children>
      <Pane prefHeight="174.0" prefWidth="338.0">
         <children>
            <Button fx:id="signup" layoutX="13.0" layoutY="128.0" mnemonicParsing="false" onAction="#signup" prefHeight="32.0" prefWidth="312.0" text="Зарегистрироваться" />
            <Label fx:id="loginLabel" layoutX="14.0" layoutY="14.0" text="Логин:" />
            <Label fx:id="passwordLabel" layoutX="14.0" layoutY="55.0" text="Пароль:" />
            <PasswordField fx:id="password" layoutX="80.0" layoutY="51.0" prefHeight="25.0" prefWidth="242.0" />
            <TextField fx:id="login" layoutX="80.0" layoutY="10.0" prefHeight="25.0" prefWidth="242.0" />
            <Button fx:id="loginSystem" layoutX="13.0" layoutY="87.0" mnemonicParsing="false" onAction="#loginSystem" prefHeight="32.0" prefWidth="312.0" text="Войти" />
         </children>
      </Pane>
   </children>
</GridPane>
